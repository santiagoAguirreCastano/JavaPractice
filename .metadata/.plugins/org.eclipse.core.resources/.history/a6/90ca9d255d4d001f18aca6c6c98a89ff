
public class Punto3 {

	
	
	public static void main(String[]args) {
		
		int num1=25;
		
		int num2=50;
		
		int resultado=num1*num2;
		
		if(resultado>1000) {
			System.out.println("Su numero es mayor a mil");
		};
		
		
		char g='c';
		
		if(g>'b') {
			
			System.out.println("Su letra está despues de la B");
		}else {
			System.out.println("Su letra está antes de la B");
		}
				
		
		
	}
}

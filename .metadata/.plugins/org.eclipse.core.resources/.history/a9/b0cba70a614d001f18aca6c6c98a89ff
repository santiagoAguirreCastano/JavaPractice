
public class Punto5 {

	public static void main(String[]args) {
		
		int num=1;
		
		do {
			System.out.println("num");
			num++;
		}while(num>=10);
		
		
int num2=0;
		
		do {
			System.out.println("num2");
			
			num2++;
			
		}while(num2>=50);
		
		
int num3=0;
		
		do {
			System.out.println("num3");
			num3=num3+num3;
			num3++;
			
		}while(num3>=50);
		
	}
}

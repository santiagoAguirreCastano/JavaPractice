
public class Punto5 {

	public static void main(String[]args) {
		
		int num=1;
		
		do {
			
			num++;
		}while(num>=10);
		System.out.println(num);
		
int num2=0;
		
		do {
			
			
			num2++;
			
		}while(num2>=50);
		
		System.out.println(num2);
int num3=0;
		
		do {
			
			num3=num3+num3;
			num3++;
			
		}while(num3>=50);
		System.out.println(num3);
	}
}
